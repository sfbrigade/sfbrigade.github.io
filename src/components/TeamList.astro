---
import TeamMember from "@/components/TeamMember.astro";
import francis from "@/assets/team/francis-li.png";
import janice from "@/assets/team/janice-tam.png";
import edward from "@/assets/team/edward-wu.jpg";
import john from "@/assets/team/john-dunning.png";
import robby from "@/assets/team/robby-taine.png";
---

<section class="my-6 flex flex-row flex-wrap items-start gap-12">
	<TeamMember
		name="Francis Li"
		title="President"
		image={francis}
	/>

	<TeamMember
		name="Janice Tam"
		title="Vice President"
		image={janice}
	/>

	<TeamMember
		name="Edward Wu"
		title="Secretary"
		image={edward}
	/>

	<TeamMember
		name="Robby Taine"
		title="Treasurer"
		image={robby}
	/>

	<TeamMember
		name="John Dunning"
		title="Board Member"
		image={john}
	/>
</section>
